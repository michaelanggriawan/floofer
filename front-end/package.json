{
  "name": "floofer",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "typecheck": "tsc --noEmit -p .",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "lint": "eslint --max-warnings 0 \"src/**/*.{ts,tsx}\"",
    "jest": "jest --coverage --passWithNoTests",
    "test": "yarn lint && yarn typecheck && yarn format:check && yarn jest"
  },
  "dependencies": {
    "exoflex": "^3.3.1",
    "@react-navigation/native": "^5.3.0",
    "@react-navigation/stack": "^5.3.3",
    "expo": "~37.0.3",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.1.tar.gz",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-safe-area-context": "0.7.3",
    "react-native-screens": "~2.2.0",
    "react-native-svg": "11.0.1",
    "react-native-web": "~0.11.7"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@types/jest": "^25.2.1",
    "@types/react": "~16.9.23",
    "@types/react-native": "~0.61.17",
    "@types/react-test-renderer": "^16.9.2",
    "babel-preset-expo": "~8.1.0",
    "eslint": "^7.0.0",
    "eslint-config-kodefox": "^0.2.2",
    "jest": "^26.0.1",
    "jest-expo": "^37.0.0",
    "prettier": "^2.0.5",
    "react-native-testing-library": "^1.13.2",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^25.5.1",
    "typescript": "^3.8.3"
  },
  "private": true,
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "eslintConfig": {
    "extends": "kodefox/react-native"
  }
}
